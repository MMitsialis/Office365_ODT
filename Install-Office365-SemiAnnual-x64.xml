	<?xml version="1.0"?>
<!--
Synopsis:
	Install :
		Office 365 ProPlus
	Architecture: 64-bit
	Install Source:
	Channel: Deferred Branch
	Updates: Deferred
	Updates Source:
	OfficeMgmtCOM : Enabled
 -->

<Configuration>
  <Add OfficeClientEdition="64" Channel="SemiAnnual" OfficeMgmtCOM="TRUE"
    <Product ID="O365ProPlusRetail">
      <Language ID="en-us"/>
    </Product>
  </Add>
  <Display Level="Full" AcceptEULA="TRUE"/>
  <Property Name="AUTOACTIVATE" Value="1"/>
  <Updates Enabled="TRUE" Channel="SemiAnnual" />
</Configuration>
